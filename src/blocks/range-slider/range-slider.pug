include ../elements-title/elements-title
include __slider/range-slider__slider

mixin range-slider(textTitle)
    .range-slider
        .range-slider__top
            +elementsTitle(textTitle)
            input.range-slider__value(type='text', value='5 000₽ - 10 000₽')
        .range-slider__buttom
            +range-slider__slider()